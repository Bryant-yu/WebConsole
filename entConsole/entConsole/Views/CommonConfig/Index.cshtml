@model entConsole.Models.CommonConfigViewModel
<form asp-controller="KeepAlive" asp-action="Index" method="get">
    @*<p>
            <select asp-for="MovieGenre" asp-items="Model.Genres">
                <option value="">All</option>
            </select>

            Title: <input type="text" name="SearchString">
            <input type="submit" value="Filter" />
        </p>*@
</form>



<meta charset="UTF-8">
<link href="~/TagJs/css/style.css" rel="stylesheet" type="text/css" />
<script src="~/TagJs/js/jquery.js"></script>
<script src="~/TagJs/js/jquery.SuperSlide.2.1.1.js"></script>









<div class="set-content">
    <div style="height: 40px;"></div>
    <div class="set-title hd">
        <ul class="clearfix">
            <li class="on">Ftp地址</li>
            <li>Http地址</li>
        </ul>
    </div>
    <div class="set-middle bd">


        <ul class="set-middle-item">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <div class="table-responsive">

                            <div class="panel panel-default">
                                <!-- Default panel contents -->
                                <div class="panel-heading"></div>

                                <table class="table" cellspacing="0" cellpadding="0" width="100%" align="center" border="0">
                                    <tr>
                                        <th>
                                            Sftp标识
                                        </th>
                                        <th>
                                            Sftp地址
                                        </th>
                                        <th>
                                            Sftp端口
                                        </th>
                                        <th>
                                            Sftp用户
                                        </th>
                                        <th>
                                            Sftp密码
                                        </th>
                                        <th>
                                            Sftp密码是否被加密
                                        </th>
                                    </tr>

                                    <tbody>
                                        @{
                                            try
                                            {
                                                foreach (var item in Model.dataInfo)
                                                {

                                                    <tr>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.Name)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.ftpUri)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.ftpPort)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.ftpUserID)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.ftpPassword)
                                                        </td>
                                                        <td>
                                                            @Html.CheckBox("slide", item.isEncrypt)
                                                        </td>
                                                    </tr>
                                                }
                                            }
                                            catch (Exception e)
                                            {
                                                @Html.ActionLink("点击查看详细错误信息", "Index", "Error", new { Msg = e.ToString() })
                                            }
                                        }
                                    </tbody>
                                </table>


                            </div>

                            @*分页标签*@
                        </div>
                    </div>
                </div>
            </div>
        </ul>




        <ul class="set-middle-item">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <div class="table-responsive">

                            <div class="panel panel-default">
                                <!-- Default panel contents -->
                                <div class="panel-heading"></div>



                                <table class="table" cellspacing="0" cellpadding="0" width="100%" align="center" border="0">
                                    <tr>
                                        <th>
                                            Https接口地址
                                        </th>
                                    </tr>

                                    <tbody>
                                        @{
                                            try
                                            {
                                                foreach (var item in Model.dataInfo)
                                                {
                                                    <tr>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.httpsAddr)
                                                        </td>
                                                    </tr>
                                                }
                                            }
                                            catch (Exception e)
                                            {
                                                @Html.ActionLink("点击查看详细错误信息", "Index", "Error", new { Msg = e.ToString() })
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>

                            @*分页标签*@
                        </div>
                    </div>
                </div>
            </div>
        </ul>



    </div>
</div>


<script>
    jQuery(".set-content").slide({ autoPlay: false, trigger: "click", delayTime: 700, pnLoop: false });
</script>


<style>
    .alt td {
        background: black !important;
    }
</style>

